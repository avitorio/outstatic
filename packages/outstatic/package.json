    "transliteration": "^2.3.5",
    "ts-pattern": "^5.5.0",
    "use-debounce": "^9.0.4",
    "zod": "^3.24.1",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
