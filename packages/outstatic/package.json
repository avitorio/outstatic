  },
  "externals": {
    "next": "next",
    "react": "react",
    "react-dom": "react-dom"
  },
  "peerDependencies": {
    "@tanstack/query-sync-storage-persister": "^5.29.0",
    "@tanstack/react-query": "5.66.0",
    "@tanstack/react-query-persist-client": "5.66.0",
    "next": "^15",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.0.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
      "prosemirror-model": "1.24.1"
    }
  }
}
