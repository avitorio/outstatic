{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "env": ["NEXT_PUBLIC_APP_URL"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "docs": {
      "dependsOn": [],
      "env": ["NEXT_PUBLIC_GA_ID", "NEXT_PUBLIC_TINYBIRD_TOKEN"]
    },
    "outstatic": {
      "dependsOn": [],
      "env": [
        "NODE_ENV",
        "OST_CONTENT_PATH",
        "OST_GITHUB_ID",
        "OST_GITHUB_SECRET",
        "OST_MONOREPO_PATH",
        "OST_REPO_BRANCH",
        "OST_REPO_SLUG",
        "OST_REPO_OWNER",
        "OST_TOKEN_SECRET",
        "VERCEL_GIT_REPO_SLUG",
        "OPENAI_API_KEY",
        "OST_GITHUB_CALLBACK_URL"
      ]
    }
  }
}
